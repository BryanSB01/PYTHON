from random import randint 

def geralista(tamanho, vMin=0, vMax=10):
    L = []
    for i in range(tamanho):
        n = randint(vMin, vMax)
        while n in L:
            n = randint(vMin, vMax)
        L.append(n)
    return L
    
def bubbleSort(Lista):
    tam = len(Lista)
    for i in range(tam - 1):
        for j in range(tam - i - 1):
            if Lista[j] > Lista[j + 1]:
                Lista[j], Lista[j + 1] = Lista[j + 1], Lista[j]
    return Lista
    
L = geralista(30)

print(f"Lista nÃ£o ordenada: {L}")
print(f"Lista ordenada: {bubbleSort(L)}")
